<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.erik.tbss.dao.PersonDao">

    <insert id="save" parameterType="org.erik.tbss.model.Person" useGeneratedKeys="true" keyProperty="recordId">
		insert
		into
		PERSON(ID,NAME)
		values(#{id},#{name})
	</insert>

</mapper>